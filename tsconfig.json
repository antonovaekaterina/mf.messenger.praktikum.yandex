{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "removeComments": false,
    "target": "es2017",
    "module": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": [
      "jest"
    ]
  }
}